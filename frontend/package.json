{
  "name": "react-typescript-boilerplate",
  "scripts": {
    "build": "set \"NODE_ENV=production\" && webpack --config ./webpack.production.js --progress",
    "start": "webpack-dev-server --config ./webpack.dev.js",
    "build:landing": "webpack --config ./webpack.landing.prod.js --progress",
    "start:landing": "webpack-dev-server --config ./webpack.landing.dev.js",
    "fix-types": "bash ./scripts/fixTypes.sh",
    "fix-fuse": "node scripts/fuseFix.js",
    "postinstall": "yarn fix-types && yarn fix-fuse",
    "test": "jest -i",
    "coverage": "codecov",
    "storybook": "node scripts/storybook.js && start-storybook -p 6006"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/es6-promise": "^0.0.33",
    "@types/es6-shim": "^0.31.32",
    "@types/fetch-mock": "^5.8.0",
    "@types/fuse": "^2.5.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.18",
    "@types/react-dom": "^16.0.2",
    "@types/react-infinite": "^0.0.30",
    "@types/react-json-pretty": "^1.3.1",
    "@types/react-scroll": "^1.5.1",
    "@types/react-test-renderer": "^16.0.0",
    "awesome-typescript-loader": "^3.1.2",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "dummy-json": "^2.0.0",
    "fetch-mock": "^5.9.4",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.3.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^16.0.0",
    "resolve-url-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "ts-jest": "^21.1.3",
    "typescript": "^2.2.1",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "@kadira/storybook": "^2.35.3",
    "@types/core-js": "^0.9.35",
    "@types/enzyme": "^3.1.1",
    "@types/history": "^4.5.0",
    "@types/jest": "^21.1.5",
    "@types/react-addons-css-transition-group": "^15.0.1",
    "@types/react-addons-transition-group": "^15.0.1",
    "@types/react-dnd": "^2.0.34",
    "@types/react-dnd-html5-backend": "^2.1.8",
    "babel-preset-env": "^1.6.1",
    "babili": "^0.1.4",
    "babili-webpack-plugin": "^0.1.2",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "enzyme": "^3.1.0",
    "es6-promise": "^4.0.5",
    "es6-promises": "^1.0.10",
    "es6-shim": "^0.35.3",
    "fetch-intercept": "^2.2.3",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mobx-react-devtools": "^4.2.15",
    "mobx-router": "^0.0.7",
    "rc-css-transition-group": "^2.1.4",
    "re-resizable": "^4.0.2",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.0.0",
    "react-infinite": "^0.12.1",
    "react-json-pretty": "^1.5.0",
    "react-markdown": "^2.4.4",
    "react-monaco-editor": "^0.11.0",
    "react-scroll": "^1.4.8",
    "reactstrap": "^5.0.0-alpha.3",
    "uglify-es": "git://github.com/mishoo/UglifyJS2#harmony",
    "xhook": "^1.4.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/api"
    ],
    "setupTestFrameworkScriptFile": "./scripts/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(scss)$": "identity-obj-proxy"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "automock": false
  }
}
